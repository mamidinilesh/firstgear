
<%= form_for(vendor) do |f| %>
<% if vendor.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(vendor.errors.count, "error") %> prohibited this region from being saved:</h2>

  <ul>
    <% vendor.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="form-horizontal">
  <div class="form-group">
    <div class="field">
      <%= f.label "Vendor Name" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :name %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Address Line 1" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :address1 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Address line 2" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :address2 %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label "State" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <select name="vendor[state_id]">
          <%= @states.each do |state| %>
          <option value="<%= state.id %>"><%= state.name %></option>
          <% end %>
        </select>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "City" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <select name="vendor[city_id]">
          <%= @cities.each do |city| %>
          <option value="<%= city.id %>"><%= city.name %></option>
          <% end %>
        </select>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Region" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <select name="vendor[region_id]">
          <%= @regions.each do |region| %>
          <option value="<%= region.id %>"><%= region.name %></option>
          <% end %>
        </select>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "lattitude" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :lat %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Longitute" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :lng %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Ranking" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :ranking %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Vendor Type" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <select name="vendor[vendor_type_id]">
          <%= @vendor_types.each do |vendor_type| %>
          <option value="<%= vendor_type.id %>"><%= vendor_type.vendor_type %></option>
          <% end %>
        </select>
      </div>
    </div>

  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Name 1" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_name_1 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Email 1" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_email_1 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Mobile 1" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_mob_1 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Name 2" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_name_2 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Email 2" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_email_2 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Contant Mobile 2" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :contact_mob_2 %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "Listing category" ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :listing_category %>
      </div>
    </div>
  </div>


  <div class="form-group">
    <div class="field">
      <%= f.label :active ,class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.check_box :active %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class=" actions col-sm-offset-2 col-sm-10">
      <%= f.submit %>
    </div>
  </div>
  <% end %>
